.content .point_name {
  margin-top: 56rpx;
}
.content .point_name .box {
  margin-top: 24rpx;
}
.content .point_name .box .top {
  width: 100%;
  height: 80rpx;
  padding: 0 28rpx;
  background: linear-gradient(180deg, #DEE5FF 0%, rgba(226, 233, 255, 0.2) 100%);
  border-radius: 8rpx 8rpx 0px 0px;
  border-bottom: 1rpx solid  #E5E5E5;
}
.content .point_name .box .top .name {
  font-size: 32rpx;
  font-weight: 500;
  color: #354052;
  line-height: 80rpx;
}
.content .point_name .box .bottom {
  width: 100%;
  padding: 24rpx 28rpx;
  background: #FFFFFF;
  border-radius: 0 0 8rpx 8rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #858B9C;
  line-height: 40rpx;
}
.content .point_name .box .bottom .value {
  color: #333333;
}
.content .point-list {
  margin-top: 56rpx;
}
.content .point-list .box {
  margin-top: 24rpx;
  width: 100%;
}
.content .point-list .box .fireFight-dev, .content .point-list .box .extra-dev {
  width: 100%;
  height: 110rpx;
  padding: 32rpx 28rpx;
  background: #FFFFFF;
  border-bottom: 1rpx solid  #E5E5E5;
  line-height: 46rpx;
}
.content .point-list .box .fireFight-dev .box-title, .content .point-list .box .extra-dev .box-title {
  margin-left: 12rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #354052;
}
.content .point-list .box .fireFight-dev .desc .box-icon, .content .point-list .box .extra-dev .desc .box-icon {
  font-size: 20rpx;
}
.content .point-list .box .fireFight-dev .name .box-icon, .content .point-list .box .extra-dev .name .box-icon {
  font-size: 38rpx;
}
.content .point-list .box .fireFight-list {
  width: 100%;
}
.content .point-list .box .fireFight-list .item {
  width: 100%;
  padding: 34rpx 28rpx;
  background: #FFFFFF;
  border-bottom: 1rpx solid  #E5E5E5;
}
.content .point-list .box .fireFight-list .item .num .name {
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
}
.content .point-list .box .fireFight-list .item .num .fix-word {
  margin-left: 10rpx;
  text-align: center;
  width: 36rpx;
  height: 36rpx;
  background: #FFF1F0;
  border-radius: 4rpx 16rpx 4rpx 4rpx;
  border: 1rpx solid #FFA39E;
  font-size: 22rpx;
  font-weight: 400;
  color: #F32C2F;
  line-height: 36rpx;
}
.content .point-list .box .fireFight-list .item .room {
  font-size: 28rpx;
  font-weight: 400;
  color: #666666;
}
.content .point-list .box .fireFight-list .item .btn {
  width: 110rpx;
  height: 54rpx;
  text-align: center;
  border-radius: 8rpx;
  border: 2rpx solid #6190E8;
  font-size: 28rpx;
  font-weight: 400;
  color: #6190E8;
  line-height: 54rpx;
}
.content .point-list .box .fireFight-dev {
  border-radius: 8rpx 8rpx 0px 0px;
}
.content .point-list .box .extra-dev {
  border-radius: 0 0 8rpx 8rpx;
}
.content .title {
  font-size: 32rpx;
  font-weight: 600;
  color: #333333;
  line-height: 45rpx;
}

