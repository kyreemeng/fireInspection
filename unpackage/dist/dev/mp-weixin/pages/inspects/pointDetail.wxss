.point-list .list0 {
  margin-top: 120rpx;
}
.point-list .list0 .item {
  width: 100%;
  background: #FFFFFF;
  border-bottom: 1rpx solid #E5E5E5;
}
.point-list .list0 .item .left {
  width: 192rpx;
  height: auto;
  padding: 16rpx 39rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #999999;
}
.point-list .list0 .item .right {
  width: 558rpx;
  border-left: 1rpx solid #E5E5E5;
  text-align: center;
  padding: 16rpx 40rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #333333;
}
.point-list .list1 {
  width: 100%;
  padding: 0 32rpx;
  margin-top: 120rpx;
}
.point-list .list1 .item {
  margin-top: 30rpx;
  width: 100%;
}
.point-list .list1 .item .title {
  width: 100%;
  height: 80rpx;
  position: relative;
}
.point-list .list1 .item .title image {
  position: absolute;
  width: 686rpx;
  height: 80rpx;
}
.point-list .list1 .item .title .name {
  position: absolute;
  margin-top: 17rpx;
  margin-left: 28rpx;
  width: 638rpx;
  font-size: 32rpx;
  font-weight: 500;
  color: #FFFFFF;
}
.point-list .list1 .item .info {
  width: 100%;
  padding: 24rpx 28rpx;
  background: #FFFFFF;
  border-radius: 0 0 8rpx 8rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #858B9C;
  line-height: 54rpx;
}
.point-list .list1 .item .info .value {
  color: #333333;
}
.point-list .bottom {
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 174rpx;
  z-index: 999;
  padding: 20rpx 30rpx;
  background: #fff;
  box-shadow: 0 -5rpx 9rpx 0 #eff3ff;
}
.point-list .bottom .btn {
  width: 100%;
  height: 100rpx;
  text-align: center;
  background: #4B87FC;
  border-radius: 8rpx;
  font-size: 36rpx;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 100rpx;
}

