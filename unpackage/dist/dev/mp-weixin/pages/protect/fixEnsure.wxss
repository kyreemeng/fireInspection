.fixensure-content .choose {
  width: 100%;
  height: 88rpx;
  background: #FFFFFF;
}
.fixensure-content .choose .picker {
  width: 375rpx;
  height: 88rpx;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #354052;
  line-height: 88rpx;
}
.fixensure-content .choose .picker .box-icon {
  font-size: 40rpx;
  margin-left: 6rpx;
}
.fixensure-content .list {
  width: 100%;
  padding: 0 32rpx;
}
.fixensure-content .list .item {
  margin-top: 30rpx;
  width: 100%;
  background: #FFFFFF;
  border-radius: 8rpx;
}
.fixensure-content .list .item .title {
  width: 100%;
  height: 80rpx;
  padding: 0 28rpx;
  border-bottom: 1rpx solid  #E5E5E5;
}
.fixensure-content .list .item .title .btn {
  width: 110rpx;
  height: 54rpx;
  border-radius: 8rpx;
  border: 2rpx solid #6190E8;
  text-align: center;
  font-size: 28rpx;
  font-weight: 400;
  color: #6190E8;
  line-height: 54rpx;
}
.fixensure-content .list .item .info {
  width: 100%;
  padding: 24rpx 28rpx;
  font-size: 28rpx;
  font-weight: 400;
  color: #858B9C;
  line-height: 54rpx;
}
.fixensure-content .list .item .info .value {
  color: #333333;
}

